{
	"settings": {
		"appearanceTools": true,
		"border": {
			"radius": "8px"
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#0071E3",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#222222",
					"name": "Text",
					"slug": "text"
				},
				{
					"color": "#f8f9fa",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				}
			]
		},
		"layout": {
			"contentSize": "840px",
			"wideSize": "1200px"
		},
		"shadow": {
			"presets": [
				{
					"name": "Soft",
					"shadow": "0 4px 12px rgba(0, 0, 0, 0.08)",
					"slug": "soft"
				},
				{
					"name": "Hover",
					"shadow": "0 8px 24px rgba(0, 0, 0, 0.12)",
					"slug": "hover"
				}
			]
		},
		"spacing": {
			"units": [
				"px",
				"rem",
				"em",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"https://rsms.me/inter/inter.css"
							]
						}
					],
					"fontFamily": "'Inter', system-ui, -apple-system, sans-serif",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "1rem",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "1.125rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.25rem",
					"slug": "large"
				},
				{
					"name": "XL",
					"size": "1.5rem",
					"slug": "xl"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "8px"
				},
				"spacing": {
					"padding": {
						"bottom": "12px",
						"left": "24px",
						"right": "24px",
						"top": "12px"
					}
				}
			},
			"woocommerce/product-image": {
				"border": {
					"radius": "8px"
				}
			},
			"woocommerce/product-price": {
				"typography": {
					"fontWeight": "600"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--text)"
		},
		"elements": {
			"button": {
				":hover": {
					"color": {
						"background": "#005bb5"
					}
				},
				"border": {
					"radius": "8px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "#ffffff"
				},
				"typography": {
					"fontWeight": "600"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "2rem",
					"fontWeight": "700"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "1.5rem",
					"fontWeight": "600"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.25rem",
					"fontWeight": "600"
				}
			}
		},
		"spacing": {
			"margin": {
				"bottom": "0",
				"top": "0"
			},
			"padding": {
				"bottom": "0",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--inter)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json"
}